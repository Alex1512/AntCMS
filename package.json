{
  "name": "antcms",
  "version": "0.0.0",
  "description": "a simple cms",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "postinstall": "tsd reinstall && typings install",
    "tscs": "gulp compile",
    "tscc": "rm -rf lib && tsc -p . --rootDir server",
    "ngc": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js && cp node_modules/core-js/client/shim.min.js aot/node_modules/ && cp node_modules/zone.js/dist/zone.js aot/node_modules",
    "map": "source-map-explorer aot/dist/build.js",
    "dev": "gulp static",
    "webpack": "NODE_ENV=development webpack --progress -p",
    "devs": "NODE_ENV=development webpack-dev-server -d --colors --hot --inline --port 4000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alex1512/AntCMS.git"
  },
  "keywords": [
    "cms",
    "react",
    "antDesign"
  ],
  "author": "kpyu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex1512/AntCMS/issues"
  },
  "homepage": "https://github.com/Alex1512/AntCMS#readme",
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/compiler-cli": "^2.1.2",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/platform-server": "^2.1.2",
    "@angular/router": "~3.1.0",
    "@angular/upgrade": "~2.1.0",
    "admin-lte": "^2.3.8",
    "angular-in-memory-web-api": "~0.1.5",
    "antd": "^1.11.0",
    "axios": "^0.14.0",
    "bluebird": "^3.4.6",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "glob": "^7.1.1",
    "immutable": "^3.8.1",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^2.2.0",
    "koa-favicon": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "koa-views": "^5.1.2",
    "lodash": "^4.16.6",
    "mongoose": "^4.6.5",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "util": "^0.10.3",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "^1.0.0-beta.3",
    "concurrently": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp-changed": "^1.3.2",
    "gulp-clean": "^0.3.2",
    "gulp-typescript": "^3.1.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "less-loader": "^2.2.3",
    "lite-server": "^2.2.2",
    "raw-loader": "^0.5.1",
    "rollup": "^0.36.3",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "source-map-explorer": "^1.3.3",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6",
    "typings": "^1.4.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0"
  }
