  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex1512/AntCMS/issues"
  },
  "homepage": "https://github.com/Alex1512/AntCMS#readme",
  "dependencies": {
    "antd": "^1.11.0",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^2.2.0",
    "koa-favicon": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "koa-views": "^5.1.2",
    "react": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "ts-loader": "^0.8.2"
  }
