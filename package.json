{
  "name": "antcms",
  "version": "0.0.0",
  "description": "a simple cms",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "postinstall": "tsd reinstall && typings install",
    "tscs": "rm -rf lib && tsc  -p ./server --rootDir ./ --removeComments",
    "tscc": "rm -rf lib && tsc -p . --rootDir server",
    "dev": "gulp static"
  },
